--refrescar y probar para volver a revisar la tabla OPERATION_HISTORY 

SELECT * FROM OPERATION_HISTORY oh WHERE oh.CAMPAIGN_ID = 100129 ORDER BY STARTING_DATE DESC;

SELECT * FROM OPERATION_HISTORY oh WHERE oh.CAMPAIGN_ID = 100129 
AND oh.STARTING_DATE >= (SYSTIMESTAMP - INTERVAL '20' MINUTE) ORDER BY STARTING_DATE DESC;

